	M3 = {10.1007/11945918{\_}35},
	Pages = {338--352},
	Title = {{Exploring Thread and Memory Placement on NUMA Architectures: Solaris and Linux, UltraSPARC/FirePlane and Opteron/HyperTransport}},
	Ty = {CHAPTER},
	Year = 2006,
